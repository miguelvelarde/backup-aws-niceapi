									"if (jsonData.Token) {",
									"    pm.environment.set(\"token\", jsonData.Token);",
									"",
									"    const savedToken = pm.environment.get(\"token\");",
									"    console.log(\"Token leído de pm.environment:\", savedToken); // <- Leer el token guardado",
									"",
									"    pm.test(\"Token guardado correctamente\", function () {",
									"        pm.expect(savedToken).to.be.a(\"string\"); // <- Validar el valor que realmente se guardó",
									"        pm.expect(savedToken).to.eql(jsonData.Token); // <- Verificar que es exactamente el mismo",
									"    });",
									"} else {",
									"    pm.test(\"Token no recibido\", function () {",
									"        pm.expect.fail(\"No se recibió un token en la respuesta\");",
